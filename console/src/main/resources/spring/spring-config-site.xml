	<bean id="duccApiUtils" class="com.jd.cjg.config.utils.DuccApiUtils">
		<property name="datacenterMap">
			<map>
				<entry key="China">
					<bean id="China" class="com.jd.cjg.config.model.cjg.Datacenter">
						<property name="duccDomain" value="${ducc.domain}"/>
