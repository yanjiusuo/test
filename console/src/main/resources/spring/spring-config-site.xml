					</bean>
				</entry>
				<entry key="Haidian">
					<bean id="Haidian" class="com.jd.cjg.config.model.cjg.Datacenter">
						<property name="duccDomain" value="${ducc.domain}"/>
						<property name="duccAppName" value="${ducc.appName}"/>
						<property name="duccAppToken" value="${ducc.appToken}"/>
					</bean>
				</entry>
				<entry key="Nongpi">
					<bean id="Nongpi" class="com.jd.cjg.config.model.cjg.Datacenter">
						<property name="duccDomain" value="${ducc.domain}"/>
						<property name="duccAppName" value="${ducc.appName}"/>
						<property name="duccAppToken" value="${ducc.appToken}"/>
					</bean>
				</entry>
				<entry key="Cbb2b">
					<bean id="Cbb2b" class="com.jd.cjg.config.model.cjg.Datacenter">
						<property name="duccDomain" value="${ducc.domain}"/>
						<property name="duccAppName" value="${ducc.appName}"/>
						<property name="duccAppToken" value="${ducc.appToken}"/>
					</bean>
				</entry>
			</map>
		</property>
	</bean>

